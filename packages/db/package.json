{
  "name": "@seasketch/db",
  "version": "0.1.0",
  "description": "SeaSketch database schema, migrations, and tests",
  "author": "Chad Burt <chad@underbluewaters.net>",
  "homepage": "https://github.com/seasketch/next",
  "license": "BSD-3-Clause",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seasketch/next.git"
  },
  "scripts": {
    "test": "jest",
    "migrate": "dotenv graphile-migrate migrate",
    "watch": "dotenv graphile-migrate watch",
    "commit": "dotenv graphile-migrate commit",
    "uncommit": "dotenv graphile-migrate uncommit",
    "reset": "dotenv graphile-migrate reset",
    "status": "dotenv graphile-migrate status",
    "psql": "docker exec -it seasketch_db psql -U postgres seasketch",
    "schema": "docker exec seasketch_db pg_dump --no-sync --schema-only --no-owner --exclude-schema=graphile_migrate --exclude-schema=postgraphile_watch -U postgres seasketch > ./schema.sql"
  },
  "bugs": {
    "url": "https://github.com/seasketch/next/issues"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.2",
    "@types/shortid": "0.0.29",
    "@types/slonik": "^22.1.2",
    "babel-jest": "^26.0.1",
    "dotenv-cli": "^3.1.0",
    "graphile-migrate": "^0.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "shortid": "^2.2.15",
    "slonik": "^22.4.8",
    "typescript": "^3.9.2"
  }
}

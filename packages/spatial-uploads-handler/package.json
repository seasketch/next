    "@turf/bbox": "^6.5.0",
    "@types/bytes": "^3.1.1",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^18.11.4",
    "@types/pg": "^8.6.5",
    "@types/tmp": "^0.2.3",
    "aws-sdk": "^2.1234.0",
    "bytes": "^3.1.2",
    "pg": "^8.8.0",
    "sanitize-filename": "^1.6.3",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "esno": "^0.16.3"
  }

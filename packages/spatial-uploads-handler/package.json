    "@turf/bbox": "^6.5.0",
    "@types/bytes": "^3.1.1",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^18.11.4",
    "@types/pg": "^8.6.5",
    "@types/tmp": "^0.2.3",
    "bytes": "^3.1.2",
    "geostats": "^2.1.0",

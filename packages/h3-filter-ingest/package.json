{
  "name": "@seasketch/h3-filter-ingest",
  "version": "1.0.0",
  "description": "Supports etl tasks related to SeaSketch's new h3 cell filtering tool",
  "author": "Chad Burt <chad@underbluewaters.net>",
  "homepage": "https://github.com/seasketch/next#readme",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seasketch/next.git"
  },
  "scripts": {
    "demo:200m-cells": "ts-node ./demo/build-cells-from-200m.ts",
    "demo:cells": "ts-node demo/build-demo-cells.ts",
    "demo:annotate": "ts-node demo/join-cells.ts output/cells-11.csv",
    "attributes": "ts-node build-attributes-json.ts output/cells-11.csv",
    "db": "ts-node build-db.ts output/attributes.json output/cells-11.csv output/db.sqlite",
    "downsample": "ts-node downsample.ts output/cells-11.csv output/attributes.json output/db.sqlite",
    "fgb": "ts-node build-fgb.ts",
    "pmtiles": "rm -rf output/*.pmtiles && ts-node build-pmtiles.ts && tile-join --force -o output/joined.pmtiles output/*.pmtiles",
    "demo:build": "npm run demo:200m-cells && npm run demo:cells && npm run demo:annotate",
    "preprocess": "npm run attributes && npm run db && npm run downsample && npm run fgb && npm run pmtiles"
  },
  "bugs": {
    "url": "https://github.com/seasketch/next/issues"
  },
  "dependencies": {
    "@seasketch/geostats-types": "^1.0.0",
    "@turf/centroid": "^7.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.5.5",
    "@types/papaparse": "^5.3.14",
    "cli-progress": "^3.12.0",
    "gdal-async": "^3.9.2",
    "h3-js": "^4.1.0",
    "papaparse": "^5.4.1",
    "parquetjs-lite": "^0.8.7",
    "simple-statistics": "^7.8.5",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2"
  }
}

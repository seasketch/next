  "scripts": {
    "build": "craco build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:serviceWorker": "source-map-explorer 'build/service-worker.js'",
    "test": "craco test",
    "eject": "react-scripts eject",
    "translation:publish": "bin/publishLocalizationTerms.js",
    "translation:import": "bin/importTranslations.js",
    "translation:sync": "npm run translation:extract && npm run translation:publish && npm run translation:import",
    "translation:extract": "rm -rf src/lang/en && NODE_ENV=development npx babel -f .babelrc 'src/**/*.{js,jsx,ts,tsx}' --ignore '**/graphql.ts,**/queries.ts' > /dev/null && mv extractedTranslations/en src/lang/en && rm -rf extractedTranslations && echo 'Translations extracted to src/lang/en'",
    "graphql:codegen": "graphql-codegen --config codegen.yml --watch 'src/**/*.tsx'",
    "start": "craco start",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "NODE_ENV=production npx eslint . --quiet",
    "deploy": "bin/deploy.sh",
    "cypress": "CYPRESS_BASE_URL=${REACT_APP_PUBLIC_URL} cypress open",
    "cypress:run": "CYPRESS_BASE_URL=${REACT_APP_PUBLIC_URL} cypress run",
    "generate-api-helpers": "node bin/buildApiHelpers.js && cp src/formElements/ExportUtils.d.ts ../api/src/"
  },

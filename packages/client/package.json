    "cypress": "CYPRESS_BASE_URL=${REACT_APP_PUBLIC_URL} cypress open",
    "cypress:run": "CYPRESS_BASE_URL=${REACT_APP_PUBLIC_URL} cypress run",
    "generate-api-helpers": "node bin/buildApiHelpers.js && cp src/formElements/ExportUtils.d.ts ../api/src/"

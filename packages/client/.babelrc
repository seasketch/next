// Note that babelrc is not used by create-react-app. Don't put stuff here!
// This is solely used to support i18next-extract
{
  "presets": ["react-app"],
  "plugins": [
    [
      "i18next-extract",
      {
        "nsSeparator": null,
        "keySeparator": false,
        "keyAsDefaultValue": ["en"],
        "useI18nextDefaultValue": true,
        "keyAsDefaultValueForDerivedKeys": true
      }
    ]
  ],
  "exclude": "src/generated/graphql.ts"
}
